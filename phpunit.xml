<?xml version="1.0" encoding="UTF-8" ?>
<phpunit bootstrap="test/bootstrap.php">
    <testsuites>
        <testsuite name="unit">
            <directory suffix="Test.php">test</directory>
        </testsuite>
    </testsuites>
    <coverage processUncoveredFiles="true">
        <include>
            <directory suffix=".php">lib</directory>
        </include>
    </coverage>
</phpunit>
